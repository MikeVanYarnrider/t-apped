{{!-- <div class="container"> --}}
  <br>
  <br>

  <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
      {{#each wellInfo.imageUrls}}
      <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
      {{/each}}
    </ol>
    <div class="carousel-inner">
      {{#each wellInfo.imageUrls}}
      <div class="carousel-item active">
        <img class="d-block w-100" src="{{this}}" alt="First slide">
      </div>
      {{/each}}
    </div>
    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>


<div class="container m-4">
 

    <main>

      <div>
        <div class="detailsRating">
          <button class="1">s</button>
          <button class="2">s</button>
          <button class="3">s</button>
          <button class="4">s</button>
          <button class="5">s</button>
        </div>
      </div>


      <h1 >{{wellInfo.name}}</h1>

      {{!-- {{#if wellInfo.averageRating >= 4}}
    <h2>This drinking fountain is very </h2>
    {{/if}} --}}


      {{!--  {{#rate wellInfo.averageRating 4}}{{/rate}} --}}
      <article >
        {{#if wellSummary}}
        <p >{{wellSummary}}{{wellAccessInfo}}{{wellAttractionInfo}}</p>
        {{/if}}
      </article>

      <div id="commentSection">
        <article class="commentEmpty">
          {{#if commentStatus}}
          <p>{{commentStatus}}</p>
          {{/if}}

        </article>
        {{#each wellInfo.comments}}
        <article class="comment">
          <h2>{{!-- {{usernamer}} --}}</h2>
          <p>{{!-- {{content}} --}}</p>
        </article>
        {{/each}}
      </div>



      <form action="/auth/login" method="POST" class="">
        {{!-- <label for="username"></label>
      <input class="form-control" id="username" type="text" name="username" placeholder="Username"> --}}
        <button class="btn btn-outline-light btn-block">Send</button>
      </form>
    </main>
</div>
